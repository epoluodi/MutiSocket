<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mutisocket"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        name="com.mutisocket.tools.App"
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/activityBG" >
        <activity
            android:name="com.mutisocket.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mutisocket.MainActivity" >
        </activity>
        <activity
            android:name="com.mutisocket.AddActivity"
            android:theme="@style/activityBG2" >
        </activity>
        <activity
            android:name="com.mutisocket.ReaderSetAty"
            android:theme="@style/activityBG3" >
        </activity>
        <activity
            android:name="com.mutisocket.OnlineReaderActivity"
            android:theme="@style/activityBG3" >
        </activity>
    </application>

</manifest>