<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="black_overlay">#66000000</color>
    <color name="blue">#FF0000FF</color>
    <color name="building">#897870</color>
    <color name="business">#5E5872</color>
    <color name="camera2">#297AED</color>
    <color name="chat">#B04B87</color>
    <color name="clean">#DED572</color>
    <color name="gray">#afafaf</color>
    <color name="green">#FF00FF00</color>
    <color name="house">#B04B87</color>
    <color name="idcard">#297AED</color>
    <color name="lightbulb">#566892</color>
    <color name="nowpublic">#E62138</color>
    <color name="police">#009B4C</color>
    <color name="shoppingcart">#2F318B</color>
    <color name="student">#12623E</color>
    <color name="traffic">#F08519</color>
    <color name="transparent">#0000</color>
    <color name="white">#FFFFFF</color>
    <string name="app_name">ReadersDemo</string>
    <string name="hello_world">Hello world!</string>
    <string name="title_activity_add">AddActivity</string>
    <string name="title_activity_main">ReadersDemo</string>
    <string name="title_activity_online_reader">OnlineReaderActivity</string>
    <string name="title_activity_reader_set_aty">ReaderSetAty</string>
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_buttom_out</item>
    </style>
    <style name="CustomProgressDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="DialogWindowTitle">
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:textAppearance">@android:style/TextAppearance.DialogWindowTitle</item>
    </style>
    <style name="MyDialogStyleBottom" parent="android:Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>
    <style name="My_Theme_Dialog_Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowTitleStyle">@style/DialogWindowTitle</item>
    </style>
    <style name="TextView"/>
    <style name="TextView.Middle">
        <item name="android:textSize">18dip</item>
    </style>
    <style name="TextView.Middle.Black">
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextView.Middle.White">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextView.Small">
        <item name="android:textSize">15dip</item>
    </style>
    <style name="TextView.Small.Black">
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextView.Small.White">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="Theme" parent="@android:style/Theme"/>
    <style name="Theme.Tabhost">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="activityBG">
        <item name="android:windowBackground">@drawable/activitybg</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="activityBG2">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@drawable/huz_popup_full_gray</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="activityBG3"> 
        <item name="android:windowFrame">@null</item> 
        <item name="android:windowIsTranslucent">true</item> 
        <item name="android:windowNoTitle">true</item>     
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="dimdialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/dialog_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="myDialogTheme" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <!-- 除去title -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <!-- <item name="android:windowBackground">@null</item>除去背景色 -->
    </style>
</resources>